{
  "name": "ganimation_package",
  "version": "2.0.0",
  "description": "A package for svg animation",
  "keywords": [
    "animation",
    "typescript"
  ],
  "homepage": "https://github.com/GhassenAbida/ganimation_package",
  "bugs": {
    "url": "https://github.com/GhassenAbida/ganimation_package/issues"
  },
  "author": "Ghassen Abida <gh.abida@outlook.com> (https://totaltypescript.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GhassenAbida/ganimation_package.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "ci": "npm run build && npm run check-format && npm run check-exports && npm run test",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "test": "vitest run",
    "dev": "vitest",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "@changesets/cli": "^2.29.4",
    "@types/flubber": "^0.4.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "flubber": "^0.4.2",
    "framer-motion": "^12.11.4",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "main": "dist/index.js",
  "peerDependencies": {
    "flubber": ">=0.4",
    "framer-motion": ">=6",
    "react": ">=17",
    "react-dom": ">=17"
  },
  "dependencies": {
    "tailwind-merge": "^3.3.0"
  }
}
